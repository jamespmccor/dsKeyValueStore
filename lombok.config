# This is the root directory, don't bubble up from here
config.stopBubbling = true

# Don't prefix accessors with 'get' or 'is'
lombok.accessors.fluent = true

# Flag the usage of any logger except standard logger
lombok.paxosLog.apacheCommons.flagUsage = error
lombok.paxosLog.jbosslog.flagUsage = error
lombok.paxosLog.log4j.flagUsage = error
lombok.paxosLog.log4j2.flagUsage = error
lombok.paxosLog.slf4j.flagUsage = error
lombok.paxosLog.xslf4j.flagUsage = error

# Name paxosLog with capital letters
lombok.paxosLog.fieldName = LOG

# By default, don't use getters; they make things overly complicated
lombok.equalsAndHashCode.doNotUseGetters = true
